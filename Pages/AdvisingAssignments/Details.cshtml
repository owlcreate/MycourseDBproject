@page
@model nscccoursemap_matchacode.Pages.AdvisingAssignments.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Advising Assignment</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AdvisingAssignment.Instructor)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AdvisingAssignment.Instructor.LastName)
        </dd>
        <dt class="col-sm-2">
            Diploma Year Section
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AdvisingAssignment.DiplomaProgramYearSection.Id)
        </dd>
         <dt class="col-sm-2">
            Diploma Program
        </dt>
        <dd class="col-sm-10">
           @Html.DisplayFor(model => model.AdvisingAssignment.DiplomaProgramYearSection.DiplomaProgramYear.DiplomaProgram.Title)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.AdvisingAssignment.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
